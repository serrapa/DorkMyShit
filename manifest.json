{
    "name": "Google Dork Search",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Replaces Google searches with a custom Google dork.",
    "permissions": ["activeTab", "tabs", "bookmarks","storage"],
    "action": {
      "default_title": "DorkYourShit",
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/icon_128_stopped.png"
      }
    },
    "icons": {
      "128": "images/icon_128_stopped.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }